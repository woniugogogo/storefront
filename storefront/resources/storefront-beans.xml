<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.storefront.data.BugData">
		<property name="title" type="String"/>
		<property name="severity" type="String"/>
		<property name="assign" type="String"/>
		<property name="desc" type="String"/>
	</bean>
	
	<bean class="de.hybris.platform.storefront.data.BugCommentData">
		<property name="date" type="String"/>
		<property name="commentContent" type="String"/>
	</bean>
	
	<bean class="de.hybris.platform.storefront.data.BugUserData">
		<property name="bugUserId" type="Integer"/>
		<property name="bugUserName" type="String"/>
	</bean>

</beans>
